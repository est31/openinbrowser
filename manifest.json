{
  "applications": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },

  "name": "Open in Browser",
  "description": "__MSG_extensionDescription__",
  "version": "2.0beta5",
  "manifest_version": 2,
  "default_locale": "en",

  "permissions": [
    "webRequest", "webRequestBlocking", "tabs",
    "downloads", "<all_urls>", "contextMenus"
  ],

  "background": {
    "scripts": ["choices.js", "background.js", "content-disposition.js"]
  }
}
